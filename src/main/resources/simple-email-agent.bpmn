<?xml version="1.0" encoding="UTF-8"?>
<bpmn:definitions xmlns:bpmn="http://www.omg.org/spec/BPMN/20100524/MODEL" xmlns:bpmndi="http://www.omg.org/spec/BPMN/20100524/DI" xmlns:dc="http://www.omg.org/spec/DD/20100524/DC" xmlns:zeebe="http://camunda.org/schema/zeebe/1.0" xmlns:di="http://www.omg.org/spec/DD/20100524/DI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:modeler="http://camunda.org/schema/modeler/1.0" id="Definitions_09w5b7d" targetNamespace="http://bpmn.io/schema/bpmn" exporter="Camunda Modeler" exporterVersion="5.43.1" modeler:executionPlatform="Camunda Cloud" modeler:executionPlatformVersion="8.8.0">
  <bpmn:process id="simple-email-agent" isExecutable="true">
    <bpmn:sequenceFlow id="Flow_0bgr4oc" sourceRef="Event_0aolqb7" targetRef="SubProcess_CommunicationAgent" />
    <bpmn:adHocSubProcess id="SubProcess_CommunicationAgent" zeebe:modelerTemplate="io.camunda.connectors.agenticai.aiagent.jobworker.v1" zeebe:modelerTemplateVersion="5" zeebe:modelerTemplateIcon="data:image/svg+xml;base64,">
      <bpmn:extensionElements>
        <zeebe:ioMapping>
          <zeebe:input source="bedrock" target="provider.type" />
          <zeebe:input source="us-east-1" target="provider.bedrock.region" />
          <zeebe:input source="credentials" target="provider.bedrock.authentication.type" />
          <zeebe:input source="{{secrets.AWS_BEDROCK_ACCESS_KEY}}" target="provider.bedrock.authentication.accessKey" />
          <zeebe:input source="{{secrets.AWS_BEDROCK_SECRET_KEY}}" target="provider.bedrock.authentication.secretKey" />
          <zeebe:input source="anthropic.claude-3-5-sonnet-20240620-v1:0" target="provider.bedrock.model.model" />
          <zeebe:input source="=5000" target="provider.bedrock.model.parameters.maxTokens" />
          <zeebe:input source="=0.2" target="provider.bedrock.model.parameters.temperature" />
          <zeebe:input source="=0.1" target="provider.bedrock.model.parameters.topP" />
          <zeebe:input source="=&#34;&#10;You are an email bot trying to satisfy business partner requests. You MUST use your own domain knowledge and the tools provided at runtime for this.&#10;&#10;Your assistant messages are NEVER seen by the business partner. Do NOT answer directly, do NOT chat, do NOT explain tools. Your job is ONLY to decide which tools to call and to call them.&#10;&#10;Use \&#34;Tool_Ask\&#34; to send any text or questions to the business partner. If you ask the business partner a question, you MUST immediately call \&#34;Tool_WaitForResponse\&#34; afterwards.&#10;&#10;If any required information is missing, use \&#34;Tool_Ask\&#34; to request it from the business partner, then call \&#34;Tool_WaitForResponse\&#34;. If you truly cannot fulfill the request even with tools, call \&#34;Tool_Ask\&#34; with a very short apology.&#10;&#10;Email context:&#10;&#10;You can leverage the following information from the incoming email to identify business partners:&#10;&#10;From: &#34;&#10;+ message.from + &#34;&#10;Name: &#34;&#10;+ message.name + &#34;&#10;&#34;&#10;" target="data.systemPrompt.prompt" />
          <zeebe:input source="=message.text" target="data.userPrompt.prompt" />
          <zeebe:input target="agentContext" />
          <zeebe:input source="in-process" target="data.memory.storage.type" />
          <zeebe:input source="=20" target="data.memory.contextWindowSize" />
          <zeebe:input source="=50" target="data.limits.maxModelCalls" />
          <zeebe:input source="WAIT_FOR_TOOL_CALL_RESULTS" target="data.events.behavior" />
          <zeebe:input source="text" target="data.response.format.type" />
          <zeebe:input source="=false" target="data.response.format.parseJson" />
          <zeebe:input source="=false" target="data.response.includeAssistantMessage" />
          <zeebe:input source="=false" target="data.response.includeAgentContext" />
          <zeebe:output source="=agent" target="agent" />
        </zeebe:ioMapping>
        <zeebe:taskDefinition type="io.camunda.agenticai:aiagent-job-worker:1" retries="3" />
        <zeebe:taskHeaders>
          <zeebe:header key="elementTemplateVersion" value="5" />
          <zeebe:header key="elementTemplateId" value="io.camunda.connectors.agenticai.aiagent.jobworker.v1" />
          <zeebe:header key="retryBackoff" value="PT0S" />
        </zeebe:taskHeaders>
        <zeebe:adHoc outputCollection="toolCallResults" outputElement="={&#10;  id: toolCall._meta.id,&#10;  name: toolCall._meta.name,&#10;  content: toolCallResult&#10;}" />
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_0bgr4oc</bpmn:incoming>
      <bpmn:outgoing>Flow_0ej315i</bpmn:outgoing>
      <bpmn:callActivity id="Tool_LoadCustomer" name="Customer">
        <bpmn:documentation>Load customer entity</bpmn:documentation>
        <bpmn:extensionElements>
          <zeebe:calledElement processId="load-customer" propagateAllChildVariables="false" propagateAllParentVariables="false" />
          <zeebe:ioMapping>
            <zeebe:input source="={&#10;  &#34;id&#34;: fromAi(value: toolCall.customerId, otpions: { required: false}),&#10;  &#34;email&#34;: fromAi(value: toolCall.customerEmailAddress, otpions: { required: false}),&#10;    &#34;fullName&#34;: fromAi(value: toolCall.customerFullName, otpions: { required: false})&#10;}" target="search" />
            <zeebe:output source="=customer" target="toolCallResult" />
          </zeebe:ioMapping>
        </bpmn:extensionElements>
      </bpmn:callActivity>
      <bpmn:receiveTask id="Tool_WaitForResponse" name="Wait for business partner response" messageRef="Message_15eqr9h">
        <bpmn:extensionElements>
          <zeebe:ioMapping />
        </bpmn:extensionElements>
      </bpmn:receiveTask>
      <bpmn:callActivity id="Tool_LoadSupplier" name="Supplier">
        <bpmn:documentation>Load supplier entity</bpmn:documentation>
        <bpmn:extensionElements>
          <zeebe:calledElement processId="load-supplier" propagateAllChildVariables="false" propagateAllParentVariables="false" />
          <zeebe:ioMapping>
            <zeebe:input source="={&#10;  &#34;id&#34;: fromAi(value: toolCall.supplierId, otpions: { required: false}),&#10;  &#34;email&#34;: fromAi(value: toolCall.supplierEmailAddress, otpions: { required: false}),&#10;    &#34;fullName&#34;: fromAi(value: toolCall.supplierFullName, otpions: { required: false})&#10;}" target="search" />
            <zeebe:output source="=customer" target="toolCallResult" />
          </zeebe:ioMapping>
        </bpmn:extensionElements>
      </bpmn:callActivity>
      <bpmn:scriptTask id="Tool_LoadDocuments" name="Load business partner documents">
        <bpmn:extensionElements>
          <zeebe:script expression="={&#10;  documentContent: &#34;blabla&#34;&#10;}" resultVariable="document" />
          <zeebe:ioMapping>
            <zeebe:input source="=fromAi(toolCall.businessPartnerId)" target="businessPartnerId" />
            <zeebe:input source="=fromAi(toolCall.documentType)" target="documentType" />
            <zeebe:output source="=document" target="toolCallResult" />
          </zeebe:ioMapping>
        </bpmn:extensionElements>
      </bpmn:scriptTask>
      <bpmn:sendTask id="Tool_Ask" name="Ask business partner" zeebe:modelerTemplate="io.camunda.connectors.message.sendtask.v1" zeebe:modelerTemplateVersion="0" zeebe:modelerTemplateIcon="data:image/svg+xml;base64,">
        <bpmn:extensionElements>
          <zeebe:ioMapping>
            <zeebe:input source="test" target="messageName" />
            <zeebe:input source="={&#10;  &#34;message&#34;: fromAi(toolCall.messageToBusinessPartner)&#10;}" target="variables" />
            <zeebe:input source="publish" target="correlationType.type" />
          </zeebe:ioMapping>
          <zeebe:taskDefinition type="io.camunda:sendMessage:1" retries="3" />
          <zeebe:taskHeaders>
            <zeebe:header key="elementTemplateVersion" value="0" />
            <zeebe:header key="elementTemplateId" value="io.camunda.connectors.message.v1" />
            <zeebe:header key="retryBackoff" value="PT0S" />
          </zeebe:taskHeaders>
        </bpmn:extensionElements>
      </bpmn:sendTask>
    </bpmn:adHocSubProcess>
    <bpmn:endEvent id="EndEvent_CaseResolved" name="Case resolved">
      <bpmn:incoming>Flow_0ej315i</bpmn:incoming>
    </bpmn:endEvent>
    <bpmn:sequenceFlow id="Flow_0ej315i" sourceRef="SubProcess_CommunicationAgent" targetRef="EndEvent_CaseResolved" />
    <bpmn:startEvent id="Event_0aolqb7" name="Email received">
      <bpmn:extensionElements>
        <zeebe:ioMapping>
          <zeebe:output source="={&#10;    &#34;id&#34;: &#34;1&#34;,&#10;    &#34;from&#34;: &#34;demo@amunda.com&#34;,&#10;    &#34;text&#34;: &#34;I need my master data documents&#34;&#10;}" target="message" />
        </zeebe:ioMapping>
      </bpmn:extensionElements>
      <bpmn:outgoing>Flow_0bgr4oc</bpmn:outgoing>
    </bpmn:startEvent>
    <bpmn:textAnnotation id="TextAnnotation_00unkcd">
      <bpmn:text>{
  from:..., 
  name:...,
  text:...
}</bpmn:text>
